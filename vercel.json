{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_ENABLE_ANALYTICS": "false"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  }
} 